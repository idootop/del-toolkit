{
  "name": "@del-wang/utils",
  "version": "1.1.0",
  "type": "module",
  "description": "🛠️ Essential utility functions for modern JavaScript development",
  "license": "MIT",
  "author": {
    "name": "Del Wang",
    "email": "hello@xbox.work",
    "url": "https://github.com/idootop"
  },
  "keywords": [
    "utils",
    "helpers",
    "is",
    "uuid",
    "debounce",
    "shallowEqual",
    "compareVersion"
  ],
  "scripts": {
    "build": "del build"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "types": "./dist/index.d.cts",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.js"
      },
      "require": {
        "types": "./dist/node/index.d.cts",
        "default": "./dist/node/index.cjs"
      }
    },
    "./web": {
      "import": {
        "types": "./dist/web/index.d.ts",
        "default": "./dist/web/index.js"
      },
      "require": {
        "types": "./dist/web/index.d.cts",
        "default": "./dist/web/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "peerDependencies": {
    "@types/node": "catalog:types"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "devDependencies": {
    "@del-wang/config": "workspace:*",
    "tsup": "catalog:dev",
    "typescript": "catalog:dev"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "packageManager": "pnpm@10.12.1",
  "homepage": "https://github.com/idootop/del-toolkit",
  "bugs": "https://github.com/idootop/del-toolkit/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idootop/del-toolkit.git"
  }
}
