{
  "name": "@del-wang/react-unstrict",
  "version": "1.0.0",
  "type": "module",
  "description": "✂️ Bypass React StrictMode double execution for consistent dev/prod behavior",
  "license": "MIT",
  "author": {
    "name": "Del Wang",
    "email": "hello@xbox.work",
    "url": "https://github.com/idootop"
  },
  "keywords": [
    "react",
    "strict-mode",
    "development",
    "useEffect",
    "hooks",
    "bypass"
  ],
  "scripts": {
    "build": "del build",
    "test": "vitest run"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "types": "./dist/index.d.cts",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0",
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@del-wang/equals": "workspace:*"
  },
  "devDependencies": {
    "@del-wang/config": "workspace:*",
    "@testing-library/dom": "catalog:test",
    "@testing-library/react": "catalog:test",
    "@types/react": "catalog:types",
    "jsdom": "catalog:test",
    "tsup": "catalog:dev",
    "typescript": "catalog:dev",
    "vitest": "catalog:test"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "packageManager": "pnpm@10.12.1",
  "homepage": "https://github.com/idootop/del-toolkit",
  "bugs": "https://github.com/idootop/del-toolkit/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idootop/del-toolkit.git"
  }
}
